{
  "name": "asi-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node18 --outfile=dibuild/index.js",
    "check:lint": "eslint . --report-unused-disable-directives",
    "check:format": "prettier --write .",
    "check:type": "tsc --noEmit",
